{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "packageManager": "pnpm@latest",
        "installCommand": "pnpm install --force",
        "buildCommand": "pnpm run build"
      }
    }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/index.html" }]
}
